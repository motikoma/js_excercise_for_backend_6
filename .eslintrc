{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:node/recommended"],
  "plugins": ["node"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2019,
  "rules": {
    "no-console": 0,
    "node/no-unsupported-features": ["error", {"version": 8}],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
  }
}
